{
  "name": "fanvise",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:integration": "RUN_INTEGRATION_TESTS=true vitest run tests/integration/**/*.test.ts",
    "test:integration:live-feeds": "RUN_INTEGRATION_TESTS=true RUN_LIVE_FEED_TESTS=true vitest run tests/integration/**/*.test.ts",
    "news:ingest": "npx tsx src/ops/news-ingest.ts",
    "news:ingest:historical": "npx tsx src/ops/news-ingest-historical.ts",
    "league:sync": "npx tsx src/ops/sync-league.ts",
    "test:ai": "cd fanvise_eval && ([ -d .venv ] || python3 -m venv .venv) && .venv/bin/python -m pip install -r requirements.txt && .venv/bin/python test_fanvise.py"
  },
  "dependencies": {
    "@google-cloud/vertexai": "^1.10.0",
    "@google/generative-ai": "^0.24.1",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-slot": "^1.2.4",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.93.3",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.30.0",
    "google-auth-library": "^10.5.0",
    "lucide-react": "^0.563.0",
    "next": "16.1.6",
    "pino": "^10.3.0",
    "postcss": "^8.5.6",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-markdown": "^10.1.0",
    "react-syntax-highlighter": "^16.1.0",
    "remark-gfm": "^4.0.1",
    "rss-parser": "^3.13.0",
    "tailwind-merge": "^3.4.0",
    "tailwindcss": "^4.1.18",
    "tailwindcss-animate": "^1.0.7",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/react-syntax-highlighter": "^15.5.13",
    "@vitest/coverage-v8": "^4.0.18",
    "dotenv": "^17.2.4",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "typescript": "^5",
    "vitest": "^4.0.18"
  }
}